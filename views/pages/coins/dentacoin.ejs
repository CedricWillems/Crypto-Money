<script>
$(document).ready(function() {
// Cryptopia
// BTC
  $("#C_DCN_BTC_details_button").click(function(){
    $(".C_DCN_BTC_details").toggleClass("hide-mobile");
    $("#C_DCN_BTC_details_button span").toggleClass('glyphicon-arrow-down').toggleClass('glyphicon-arrow-up');
  })

// LTC
  $("#C_DCN_LTC_details_button").click(function(){
    $(".C_DCN_LTC_details").toggleClass("hide-mobile");
    $("#C_DCN_LTC_details_button span").toggleClass('glyphicon-arrow-down').toggleClass('glyphicon-arrow-up');
  })

// DOGE
  $("#C_DCN_DOGE_details_button").click(function(){
    $(".C_DCN_DOGE_details").toggleClass("hide-mobile");
    $("#C_DCN_DOGE_details_button span").toggleClass('glyphicon-arrow-down').toggleClass('glyphicon-arrow-up');
  })

  
// Coin Market Cap
// BTC
  $("#CMC_DCN_BTC_details_button").click(function(){
    $(".CMC_DCN_BTC_details").toggleClass("hide-mobile");
    $("#CMC_DCN_BTC_details_button span").toggleClass('glyphicon-arrow-down').toggleClass('glyphicon-arrow-up');
  })

// DCN_BTC
  var DCN_BTC_Historical_Data_date = [];
  var DCN_BTC_Historical_Data_price = [];

<%for (var i = C_DCN_BTC_CHART.length - 1; i >= 0; i-= 5) {
  %>
    DCN_BTC_Historical_Data_date.push(moment(<%=C_DCN_BTC_CHART[i].Timestamp*1000%>).format("YYYY-MM-DD HH:mm"));      
    DCN_BTC_Historical_Data_price.push(<%=(CC_BTC_EUR*(C_DCN_BTC_CHART[i].Price*DCN)).toFixed(2)%>);
  <%
}
%>

  /*console.log("BTC");
  console.log(DCN_BTC_Historical_Data_date);
  console.log(DCN_BTC_Historical_Data_price);*/

  var ctx = document.getElementById("DCN_BTC_CHART").getContext('2d');
  var myChart = new Chart(ctx, {
      type: 'line',
      data: { 
        labels: DCN_BTC_Historical_Data_date,        
          datasets: [{
              label: 'Price',
              data: DCN_BTC_Historical_Data_price.reverse(),
              backgroundColor: [
                'rgba(54, 162, 235, 0.2)',
              ],
              borderColor: [
                'rgba(54, 162, 235, 1)',
              ],
              borderWidth: 1
          }]
      },
      options: 
      {
        responsive: true,
        maintainAspectRatio: false,
        scales: {
          yAxes: [{
            scaleLabel: {
              display: true,
              labelString: 'Price in Euro (All DCN)'
            }
          }],
          xAxes: [{
            scaleLabel: {
              display: true,
              labelString: 'Date'
            },
            type: 'time',
            time: {
               unit: 'hour'
            }
          }]
        }
      }
  });

// DCN_LTC
  var DCN_LTC_Historical_Data_date = [];
  var DCN_LTC_Historical_Data_price = [];

  <%for (var i = 0; i < C_DCN_LTC_CHART.length; i++) {
    %> 
      DCN_LTC_Historical_Data_date.push(moment(<%=C_DCN_LTC_CHART[i].Timestamp*1000%>).format("YYYY-MM-DD HH:mm"));      
      DCN_LTC_Historical_Data_price.push(<%=(CC_LTC_EUR*(C_DCN_LTC_CHART[i].Price*DCN)).toFixed(2)%>);
    <%
  }
  %>

  /*console.log("LTC");
  console.log(DCN_LTC_Historical_Data_date);
  console.log(DCN_LTC_Historical_Data_price);*/

  var ctx = document.getElementById("DCN_LTC_CHART").getContext('2d');
  var myChart = new Chart(ctx, {
      type: 'line',
      data: { 
        labels: DCN_LTC_Historical_Data_date,        
          datasets: [{
              label: 'Price',
              data: DCN_LTC_Historical_Data_price,
              backgroundColor: [
                'rgba(54, 162, 235, 0.2)',
              ],
              borderColor: [
                'rgba(54, 162, 235, 1)',
              ],
              borderWidth: 1
          }]
      },
      options: 
      {
        responsive: true,
        maintainAspectRatio: false,
        scales: {
          yAxes: [{
            scaleLabel: {
              display: true,
              labelString: 'Price in Euro (All DCN)'
            }
          }],
          xAxes: [{
            scaleLabel: {
              display: true,
              labelString: 'Date'
            },
            type: 'time',
            time: {
               unit: 'hour'
            }
          }]
        }
      }
  });

// DCN_DOGE
  var DCN_DOGE_Historical_Data_date = [];
  var DCN_DOGE_Historical_Data_price = [];

  <%for (var i = 0; i < C_DCN_DOGE_CHART.length; i++) {
    %> 
      DCN_DOGE_Historical_Data_date.push(moment(<%=C_DCN_DOGE_CHART[i].Timestamp*1000%>).format("YYYY-MM-DD HH:mm"));      
      DCN_DOGE_Historical_Data_price.push(<%=(CC_DOGE_EUR*(C_DCN_DOGE_CHART[i].Price*DCN)).toFixed(2)%>);
    <%
  }
  %>

  /*console.log("DOGE");
  console.log(DCN_DOGE_Historical_Data_date);
  console.log(DCN_DOGE_Historical_Data_price);*/

  var ctx = document.getElementById("DCN_DOGE_CHART").getContext('2d');
  var myChart = new Chart(ctx, {
      type: 'line',
      data: { 
        labels: DCN_DOGE_Historical_Data_date,        
          datasets: [{
              label: 'Price',
              data: DCN_DOGE_Historical_Data_price,
              backgroundColor: [
                'rgba(54, 162, 235, 0.2)',
              ],
              borderColor: [
                'rgba(54, 162, 235, 1)',
              ],
              borderWidth: 1
          }]
      },
      options: 
      {
        responsive: true,
        maintainAspectRatio: false,
        scales: {
          yAxes: [{
            scaleLabel: {
              display: true,
              labelString: 'Price in Euro (All DCN)'
            }
          }],
          xAxes: [{
            scaleLabel: {
              display: true,
              labelString: 'Date'
            },
            type: 'time',
            time: {
               unit: 'hour'
            }
          }]
        }
